{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
        {
            "type": "node-terminal",
            "name": "运行脚本: dev",
            "request": "launch",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}/part3/learn/"
        }
    ]
}